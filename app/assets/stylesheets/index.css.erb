  .wrapper{
    height: 100%;
    width: 100%;
    text-align: center;
  }
  .contentwidth{
    width: 960px;
    display: inline-block;
    vertical-align: middle;
  }
  .header{
    color: #fff;
    height: 100%;
    background-color: #eee;
    background: url('<%= asset_path("milkbottle.jpg") %>');
    -webkit-background-size: cover;
    background-size: cover;
    position: relative;
  }
  .header:before{
    content: "";
    display: inline-block;
    height: 100%;
    vertical-align: middle;
  }
  .header h1{
    font-size: 200px;
    text-shadow: 1px 1px 2px #fff,
                 2px 2px 2px #ddd,
                 3px 3px 2px #bbb,
                 4px 4px 2px #999,
                 5px 5px 2px #777,
                 6px 6px 2px #555,
                 7px 7px 2px #333,
                 8px 8px 2px #111;
    margin-bottom: 20px;
  }
  .header p{
    font-size: 26px;
    text-shadow: 3px 3px #666;
    line-height: 30px;
    margin-bottom: 5px;
  }
  .header a{
    color: #fff;
    text-decoration: none;
    cursor: default;
    position: absolute;
    bottom: 50px;
    left: 50%;
    margin-left: -32px;
  }
  .header a:before{
    content: "";
    width: 30px;
    height: 30px;
    border: 2px solid #fff;
    border-radius: 8px;
    position: absolute;
    left: 50%;
    margin-left: -17px;
    top: 25px;
  }
  .header a:after{
    content: "";
    width: 11px;
    height: 11px;
    border: 5px solid #fff;
    border-width: 0 2px 2px 0;
    -ms-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    position: absolute;
    left: 50%;
    margin-left: -7px;
    top: 32px;
  }
  .direct_to{
    height: 500px;
    background-color: #fff;
  }
  .direct_to:before{
    content: "";
    display: inline-block;
    height: 100%;
    vertical-align: middle;
  }
  .direct_to:after{
    content: "";
    display: block;
    position: absolute;
    width: 0;
    height: 0;
    border: 30px solid;
    border-color: #fff transparent transparent transparent;
    left: 50%;
    margin-left: -15px;
  }
  .direct_to p{
    font-size: 36px;
  }
